{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
		"jsx-no-multiline-js": false,
    "prefer-array-literal": false,
    "jsx-boolean-value": false,
    "jsx-no-lambda": false,
    "import-name": [false],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "avoid-template"
    ]
  },
  "rulesDirectory": []
}